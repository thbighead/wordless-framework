{
  "name": "infobaseit/wordless-framework",
  "description": "Wordless Framework.",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Wordless\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wordless\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Thales Nathan",
      "email": "thales.matias@infobase.com.br"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-openssl": "*",
    "doctrine/inflector": "^2.0",
    "lcobucci/jwt": "^4.2",
    "monolog/monolog": "^2.9",
    "ramsey/uuid": "^4.2",
    "symfony/console": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/http-client": "^5.3",
    "symfony/http-foundation": "^5.3",
    "symfony/validator": "^5.4",
    "wp-cli/wp-cli-bundle": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "psy/psysh": "^0.10.8",
    "symfony/var-dumper": "^5.3"
  }
}
